local function GetInstanceWithTime(Parent: Instance, Class: string, Name: string | nil, Timeout: number)
	local CurTime = 0

	while Timeout > CurTime do
		for _, v in GetChildren(Parent) do
			if IsA(v, Class) and (not Name or v.Name == Name) then
				return v
			end
		end

		CurTime += task.wait(0)
	end
end

